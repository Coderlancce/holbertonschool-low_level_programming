int **alloc_grid(int width, int height)
{
  int **a;
  int i = 0, j = 0;
  
  if (width <= 0 || height <= 0)
    return (NULL);

  while (i < width)
    i++;

  while (j < heigth)
    j++;

  for(j = 0; j < heigth; j++)
    {
      for (i = 0; i < width; i++)
	if (
	a[j][i] = malloc(sizeof(int) * width);
      
      }
    
}
